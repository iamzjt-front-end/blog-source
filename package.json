  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node scripts/unpack.ts",
    "dev:admin": "cross-env NODE_ENV=development NODE_PROJECT_ENV=admin ts-node scripts/unpack.ts",
    "dev:all": "npm-run-all --parallel dev dev:admin",
    "build": "cross-env NODE_ENV=production ts-node scripts/unpack.ts",
    "build:admin": "cross-env NODE_ENV=production BUILD_ENV=admin ts-node scripts/unpack.ts",
    "build:client": "cross-env NODE_ENV=production BUILD_ENV=client ts-node scripts/unpack.ts",
    "build:server": "cross-env NODE_ENV=production BUILD_ENV=server ts-node scripts/unpack.ts",
    "preview": "npm-run-all --parallel preview:admin preview:client",
    "preview:admin": "cross-env NODE_PREVIEW=true NODE_ENV=production BUILD_ENV=admin ts-node scripts/unpack.ts",
    "preview:client": "cross-env NODE_PREVIEW=true NODE_ENV=production BUILD_ENV=client ts-node scripts/unpack.ts",
    "start": "cross-env NODE_ENV=production ts-node scripts/start.ts",
    "start:pm2": "cross-env NODE_ENV=production pm2 start pm2.json",
    "restart:pm2": "cross-env NODE_ENV=production pm2 restart pm2.json",
    "prod": "npm run build && npm run start",
    "prod:pm2": "npm run build && npm run start:pm2",
    "prod:restart": "npm run build && npm run restart:pm2",
    "commit": "cz"
  },
